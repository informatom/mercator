function category() {
  $('#category').w2grid({
    name: 'category',
    method: 'GET',
    columns : [
      {
        field: 'attribute',
        caption: '<%= I18n.t("attributes.attrib") %>',
        size: '30%'
      },
      {
        field: 'value',
        caption: '<%= I18n.t("attributes.value") %>',
        size: '70%'
      }
    ]
  })
}