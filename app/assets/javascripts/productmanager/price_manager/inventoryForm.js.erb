function inventoryForm() {
  $('#inventoryForm').w2form({
    name: 'inventoryForm',
    url: '/productmanager/price_manager/manage_inventory',

    formHTML:
      '<div class="w2ui-page page-0">'+
      '  <div style="float: left">'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.name") %> DE</label>'+
      '      <div><input name="name_de" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.name") %> EN</label>'+
      '      <div><input name="name_en" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.comment") %> DE</label>'+
      '      <div><input name="comment_de" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.comment") %> EN</label>'+
      '      <div><input name="comment_en" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.number") %></label>'+
      '      <div><input name="number" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("activerecord.attributes.inventory.alternative_number") %></label>'+
      '      <div><input name="alternate_number" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.infinite") %></label>'+
      '      <div><input name="infinite" type="checkbox"/></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.amount") %></label>'+
      '      <div><input name="amount" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.unit") %></label>'+
      '      <div><input name="unit" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.weight") %></label>'+
      '      <div><input name="weight" /></div>'+
      '    </div>'+
      '  </div>'+
      '  <div style="float: right; margin-right:20px;">'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.charge") %></label>'+
      '      <div><input name="charge" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("activerecord.attributes.inventory.storage") %></label>'+
      '      <div><input name="storage" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.delivery_time") %></label>'+
      '      <div><input name="delivery_time" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.created_at") %></label>'+
      '      <div><input name="created_at" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label><%= I18n.t("attributes.updated_at") %></label>'+
      '      <div><input name="updated_at" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label>ERP <%= I18n.t("attributes.updated_at") %></label>'+
      '      <div><input name="erp_updated_at" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label>ERP <%= I18n.t("attributes.vat") %></label>'+
      '      <div><input name="erp_vatline" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label>ERP <%= I18n.t("attributes.category") %></label>'+
      '      <div><input name="erp_article_group" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label>ERP <%= I18n.t("activerecord.attributes.inventory.provision_code") %></label>'+
      '      <div><input name="erp_provision_code" /></div>'+
      '    </div>'+
      '    <div class="w2ui-field">'+
      '      <label>ERP Flag</label>'+
      '      <div><input name="erp_characteristic_flag" /></div>'+
      '    </div>'+
      '  </div>'+
      '</div>'+
      '<div class="w2ui-buttons">'+
      '   <button class="w2ui-btn w2ui-btn-red" name="delete">'+
      '     <%= I18n.t("hobo.actions.delete", model: Inventory.model_name.human) %>'+
      '   </button>'+
      '   <button class="w2ui-btn w2ui-btn-green" name="save">'+
      '     <%= I18n.t("hobo.actions.save", model: Inventory.model_name.human) %>'+
      '   </button>'+
      '   <button class="w2ui-btn w2ui-btn-orange" name="create">'+
      '   <%= I18n.t("hobo.actions.create", model: Inventory.model_name.human) %>'+
      '   </button>'+
      '</div>',

    fields : [
      { name: 'name_de', type: 'text', required : true},
      { name: 'name_en', type: 'text' },
      { name: 'comment_de', type: 'text' },
      { name: 'comment_en', type: 'text' },
      { name: 'number', type: 'text', required : true },
      { name: 'alternate_number', type: 'text' },
      { name: 'infinite', type: 'checkbox'},
      { name: 'amount', type: 'float', required : true },
      { name: 'unit', type: 'text', required : true },
      { name: 'weight', type: 'float' },

      { name: 'charge', type: 'text' },
      { name: 'storage', type: 'text' },
      { name: 'delivery_time', type: 'list', required : true,
        options: { items: [ <% Constant.where(key: 'delivery_times_de').first.value.split(',').each do |option| %>
                              { id: '<%= option %>', text: '<%= option %>'},
                            <% end %> ] } },
      { name: 'created_at', type: 'text', disabled: true },
      { name: 'updated_at', type: 'text', disabled: true },
      { name: 'erp_updated_at', type: 'text', disabled: true },
      { name: 'erp_vatline', type: 'text', disabled: true },
      { name: 'erp_article_group', type: 'text', disabled: true },
      { name: 'erp_provision_code', type: 'text', disabled: true },
      { name: 'erp_characteristic_flag', type: 'text', disabled: true }
    ],

    actions: {
      save: {
        caption: 'Save',
        class: 'w2ui-btn-green',
        onClick: function () {
        }
      },
      delete: {
        caption: 'Delete',
        class: 'w2ui-btn-red',
        onClick: function () {
        }
      },
      create: {
        caption: 'Add New',
        class: 'w2ui-btn-orange',
        onClick: function () {
        }
      }
    }
  })
}