function productForm() {
  $('#productForm').w2form({
    name: 'productForm',
    recid: request_id,
    url: '/productmanager/price_manager/manage_product',
    fields : [
      {
        name: 'number',
        type: 'text',
        html: { caption: '<%= I18n.t("attributes.number") %>' }
      }, {
        name: 'state',
        type: 'text',
        html: { caption: '<%= I18n.t("attributes.state") %>' }
      }, {
        name: 'description_de',
        type: 'text',
        html: {
          caption: '<%= I18n.t("attributes.description") %> DE' ,
          attr: 'size=40'
        }
      }, {
        name: 'description_en',
        type: 'text',
        html: {
          caption: '<%= I18n.t("attributes.description") %> EN',
          attr: 'size=40'
        }
      }, {
        name: 'long_description_de',
        type: 'textarea',
        html: {
          caption: '<%= I18n.t("attributes.long_description") %> DE',
          attr: 'rows=15 cols=40'
        }
      }, {
        name: 'long_description_en',
        type: 'textarea',
        html: {
          caption: '<%= I18n.t("attributes.long_description") %> EN',
          attr: 'rows=15 cols=40'
        }
      }, {
        name: 'warranty_de',
        type: 'text',
        html: {
          caption: '<%= I18n.t("attributes.warranty") %> DE',
          attr: 'size=40'
        }
      }, {
        name: 'warranty_en',
        type: 'text',
        html: {
          caption: '<%= I18n.t("attributes.warranty") %> EN',
          attr: 'size=40'
        }
      }, {
        name: 'novelty',
        type: 'checkbox',
        html: { caption: '<%= I18n.t("attributes.novelty") %>' }
      }, {
        name: 'topseller',
        type: 'checkbox',
        html: { caption: '<%= I18n.t("attributes.topseller") %>' }
      }, {
        name: 'created_at',
        type: 'text',
        html: { caption: '<%= I18n.t("attributes.created_at") %>' },
        disabled: true
      }, {
        name: 'updated_at',
        type: 'text',
        html: { caption: '<%= I18n.t("attributes.updated_at") %>' },
        disabled: true
      }
    ],

    actions: {
      save: {
        caption: 'Save',
        class: 'w2ui-btn-green',
        onClick: function () {
        }
      },
      delete: {
        caption: 'Delete',
        class: 'w2ui-btn-red',
        onClick: function () {
        }
      },
      create: {
        caption: 'Add New',
        class: 'w2ui-btn-orange',
        onClick: function () {
        }
      }
    }
  })
}