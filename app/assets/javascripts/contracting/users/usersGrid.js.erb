function usersGrid(domelement, url) {
  if (w2ui[domelement] != undefined) {
    w2ui[domelement].destroy()
  }

  $('#' + domelement).w2grid({
    name: domelement,
    multiSelect : false,
    method: 'GET',
    show: { toolbar: true, footer: true },

    columns: [
      { field: 'recid', caption: I18n.t("attributes.id"), size: '50px', sortable: true, attr: "align=right" },
      { field: 'gender', caption: I18n.t("attributes.gender"), size: '50px', sortable: true },
      { field: 'title', caption: I18n.t("attributes.title"), size: '50px', sortable: true },
      { field: 'first_name', caption: I18n.t("attributes.first_name"), size: '100px', sortable: true },
      { field: 'surname', caption: I18n.t("attributes.surname"), size: '100px', sortable: true },
      { field: 'email_address', caption: I18n.t("attributes.email_address"), size: '250px', sortable: true },
      { field: 'phone', caption: I18n.t("attributes.phone"), size: '150px', sortable: true },
      { field: 'administrator', caption: "Administrator", size: '50px', sortable: true,
        render: 'toggle' },
      { field: 'sales', caption: I18n.t("attributes.sales"), size: '50px', sortable: true,
        render: 'toggle' },
      { field: 'sales_manager', caption: I18n.t("attributes.sales_manager"), size: '50px', sortable: true,
        render: 'toggle' },
      { field: 'contentmanager', caption: I18n.t("attributes.contentmanager"), size: '50px', sortable: true,
        render: 'toggle' },
      { field: 'productmanager', caption: I18n.t("attributes.productmanager"), size: '50px', sortable: true,
        render: 'toggle' },
      { field: 'last_login_at', caption: I18n.t("attributes.last_login_at"), size: '50px', sortable: true },
      { field: 'logged_in', caption: I18n.t("attributes.logged_in"), size: '50px', sortable: true,
        render: 'toggle' },
      { field: 'login_count', caption: I18n.t("attributes.login_count"), size: '50px', sortable: true },
      { field: 'gtc_confirmed_at', caption: I18n.t("attributes.gtc_confirmed_at"), size: '50px', sortable: true },
      { field: 'gtc_version_of', caption: I18n.t("attributes.gtc_version_of"), size: '50px', sortable: true },
      { field: 'erp_account_nr', caption: "ERP Account #", size: '50px', sortable: true },
      { field: 'erp_contact_nr', caption: "ERP Contact #", size: '50px', sortable: true },
      { field: 'locale', caption: I18n.t("attributes.locale"), size: '50px', sortable: true },
      { field: 'call_priority', caption: I18n.t("attributes.call_priority"), size: '50px', sortable: true, hidden: true },
      { field: 'waiting', caption: I18n.t("attributes.waiting"), size: '50px', sortable: true, hidden: true,
        render: 'toggle' },
      { field: 'editor', caption: I18n.t("attributes.editor"), size: '50px', sortable: true, hidden: true },
      { field: 'created_at', caption: I18n.t("attributes.created_at"), size: '130px', sortable: true,
        render: 'datetime:dd.mm.yyyy|hh24:mm:ss' },
      { field: 'updated_at', caption: I18n.t("attributes.updated_at"), size: '130px', sortable: true,
        render: 'datetime:dd.mm.yyyy|hh24:mm:ss' },
    ],

    searches: [
      { field: 'gender', caption: I18n.t("attributes.gender"), type: 'text' },
      { field: 'title', caption: I18n.t("attributes.title"), type: 'text' },
      { field: 'first_name', caption: I18n.t("attributes.first_name"), type: 'text' },
      { field: 'surname', caption: I18n.t("attributes.surname"), type: 'text' },
      { field: 'email_address', caption: I18n.t("attributes.email_address"), type: 'text' },
      { field: 'phone', caption: I18n.t("attributes.phone"), type: 'text' },
      { field: 'erp_account_nr', caption: "ERP Account #", type: 'text' },
      { field: 'erp_contact_nr', caption: "ERP Contakt #", type: 'text' }
    ],
    sortData: [{ field: 'email_address', direction: 'asc' }]
  })

  w2ui[domelement].load(url)
  w2ui[domelement].refresh()
}