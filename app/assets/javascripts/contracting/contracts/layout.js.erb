$(function () {
  if (I18n.locale == "de") {
    w2utils.locale('<%= asset_path('contentmanager/de-de.json') %>')
  }
  w2utils.settings.groupSymbol = " "

  $('#contracting').w2layout({
    name: 'contracting',
    panels: [
      { type: 'top', size: 59, resizable: true, title: I18n.t("js.con.title"),
        content: "<div id='menu'></div>" },
      { type: 'main', title: I18n.t("js.con.Contract.other"),
        resizable: true, content: "<div id='contractsGrid' style='height:100%;'></div>" }
    ],
    onRender: function(event) {
      event.onComplete = function () {
        contractsGrid()
        menu()
      }
    }
  })
})