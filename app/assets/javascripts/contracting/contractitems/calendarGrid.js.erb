function calendarGrid() {
  $('#calendarGrid').w2grid({
    name: 'calendarGrid',
    multiSelect : false,
    method: 'GET',
    show: { toolbar: false, footer: true },

    columns: [
      { field: 'title', size: '200px', style: 'font-weight: bold' },
      { field: 'year1', caption: I18n.t("attributes.year") + ' 1', size: '100px' },
      { field: 'year2', caption: I18n.t("attributes.year") + ' 2', size: '100px' },
      { field: 'year3', caption: I18n.t("attributes.year") + ' 3', size: '100px' },
      { field: 'year4', caption: I18n.t("attributes.year") + ' 4', size: '100px' },
      { field: 'year5', caption: I18n.t("attributes.year") + ' 5', size: '100px' },
      { field: 'year6', caption: I18n.t("attributes.year") + ' 6', size: '100px' },
    ],
  })
}