<extend tag="form" for="ShippingCost">
  <old-form merge>
   <field-list:>
      <country-view:>
        <name-one class="form-control" complete-target="&Country.new()" completer="name_de_admin" autocomplete="off"/>
      </country-view:>
      <shipping-method-view:>
        <select-menu class="form-control" name="shipping_cost[shipping_method]" options="&['parcel_service_shipment']"/>
      </shipping-method-view:>
   </field-list:>
  </old-form>
</extend>